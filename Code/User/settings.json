{
    "workbench.colorTheme": "Quiet Light",
    "[cpp]": {},
    "terminal.external.linuxExec": "urxvt",
    "files.autoSave": "afterDelay",
    "editor.fontSize": 13,
    "editor.fontFamily": "'Inconsolata', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 10000,
    "editor.codeLens": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.defaultDateFormat": "DD-MMM-YYYY  HH:MM",
    "gitlens.historyExplorer.enabled": true,
    "gitlens.gitExplorer.files.layout": "list",
    "gitlens.keymap": "alternate",
    "python.pythonPath": "/usr/local/bin/python",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.errorSquiggles": "Disabled",
    "C_Cpp.loggingLevel": "None",
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "cquery.completion.include.whitelistLiteralEnding": [
        ".h",
        ".hpp",
        ".hh",
        ".hxx"
    ],
    "cquery.misc.indexerCount": 7,
    "explorer.confirmDelete": false,
    "rust-client.channel": "stable",
    "rust-client.logToFile": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        ".vscode/": true,
        ".git/": true
    },
    "files.associations": {
        "*.in": "makefile"
    },
    "window.zoomLevel": 0,
}