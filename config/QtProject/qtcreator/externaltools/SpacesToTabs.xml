<externaltool id="SpacesToTabs">
    <description>Replace 4 spaces with 1 TAB in the selected text</description>
    <displayname>SpacesToTabs</displayname>
    <category>Text</category>
    <executable output="replaceselection" error="ignore" modifiesdocument="yes">
        <path>unexpand</path>
        <arguments>-t 4</arguments>
        <input>%{CurrentDocument:Selection}</input>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
