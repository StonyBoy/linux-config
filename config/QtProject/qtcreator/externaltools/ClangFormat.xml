<externaltool id="ClangFormat">
    <description>clang format</description>
    <displayname>Clang Format</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>clang-format</path>
        <arguments>-style=file -i %{CurrentDocument:FileName}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
